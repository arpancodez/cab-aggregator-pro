{
  "name": "cab-aggregator-backend",
  "version": "1.0.0",
  "description": "Premium cab price aggregator - Node/Express backend API",
  "main": "dist/app.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": ["cab", "aggregator", "ride", "uber", "ola", "rapido", "api"],
  "author": "Arpan",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^5.0.0",
    "axios": "^1.5.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.1.0",
    "bcrypt": "^5.1.1",
    "socket.io": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
